<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.neo.mapper.OnlineBugMapper">
	
	<!-- findAll -->
	<select id="findAll" resultType="OnlineBugPojo">
		select * from onlinebug
	</select>
	
	<!-- findCount -->
	<select id="findCount" resultType="int" >
		select count(1) from onlinebug
	</select>
	
	<!-- findOne -->
	<select id="findOne" resultType="OnlineBugPojo">
		select * from onlinebug where id=#{value}
	</select>
	
	<!-- save -->
	<insert id="save" parameterType="OnlineBugPojo">
		insert into onlinebug (id,name,cbs,capp,ch5,cweb,deal,agent)
		values(#{id}, #{name}, #{cbs}, #{capp}, #{ch5}, #{cweb},#{deal},#{agent})
	</insert>
	
</mapper>